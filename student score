#include<stdio.h>
int main()
{
	int phy,chem,bio,math,comp;
	float per;
	printf("enter four subjects marks:");
	scanf("%d%d%d%d",&phy, &chem, &bio, &math, &comp);
	per=(phy + chem+ bio+ math)/4.0;
	printf("percentage=%.2f\n",per);
    if(per>=34)
	{
	printf("distinction");	
	}
	else if(per>=50)
	{
		printf("first calss");
	}
	else if(per>=75)
	{
		printf("second class");
	}
	else if(per>=60)
	{
		printf("third class");
	}
	else 
	{
		printf("fail");
	}
	return 0;
}
