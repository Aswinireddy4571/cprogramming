#include<stdio.h>
int main()
{
	int i,j,n;
	/*input number of rows from user*/
	printf("enter number of rows:");
	scanf("%d",&n);
	/*iterate through n rows*/
	for(i=1;i<=n;i++)
	{
		/*interate over columns*/
		for(j=1;j<=n;j++)
		{
			/*print star for each column*/
			printf("*");
		}
		/*move to the nt line/row*/
		printf("\n");
	}
	return 0;
}
